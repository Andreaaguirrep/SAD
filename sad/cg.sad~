FFS;

w=KBMainFrame["CP",f];
c=Canvas[f,Width->800,Height->600];
Canvas$Widget=c;

contl=1;
ListContourPlot;

data = Table[Sin[x]/Cos[x^2 + y^2], {x, (-2), 2, 0.1}, {y, (-2), 2, 0.1}]; 
Timing[g=ListContourPlot[data,PlotRange->{-5,5},
  MeshRange->{{-2,2},{-2,2}},ContourColorFunction->Automatic,
  AspectRatio->0.9,
  ColorFunction->Automatic,DisplayFunction->Identity,Contours->10,
  FrameLabel->{"x","y"}]]
Timing[Show[g]]
Update[];

end

data=Table[-1/Sqrt[(x-1)^2+y^2+0.1]-1/Sqrt[(x+1)^2+y^2+0.1]-1/Sqrt[x^2+(y+1)^2+0.1],
  {x,-3,3,0.2},{y,-3,3,0.2}];

g=ListContourPlot[data,
  MeshRange->{{-3,3},{-3,3}},ContourColorFunction->Automatic,
  ColorFunction->Automatic,DisplayFunction->Identity,Contours->10,
  FrameLabel->{"x","y"}];
Show[g];
Update[];

end


