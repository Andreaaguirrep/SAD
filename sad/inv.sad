Norm[m_]:=Module[{tm=Transpose[m],l,r,{lm,wm}=Dimensions[m]},
  l=Min[lm,wm];
  r=Sqrt[MapThread[Max[Abs[#]]/Max[1e-10,Abs[#2]]&,{Take[m,l],Take[tm,l]}]];
  r1=Join[r,Table[0,{lm-l}]];
  r2=Join[r,Table[0,{wm-l}]];
  ((#*r2)&/@m)/r1]

