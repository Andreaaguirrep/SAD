OFF LOG CTIME ;ON ECHO;

MULT M1=(L=1 K0=0.1 K3=1)
;
SOL ES1=(GEO=1 BOUND=1)
    ES2=(BOUND=1)
;
MARK ENT=()
     CM1=(OFFSET=1.5)
;
LINE BT=(ENT ES1 CM1 M1 ES2)
;
FFS USE=BT;
!FFS$NumericalDerivative=True;
ins trpt;
cal;

angle=0.1;

fit $$$ chi1 angle/Degree;
fit cm1 dpx 0 dx 0;
free m1 k0 es1 dpx es1 dx;

go

line=ExtractBeamLine[BT];
SetElement["QF","QUAD",{"L"->1,"K1"->0.1}];
line1=Insert[line,QF,3];
use line1;

end
