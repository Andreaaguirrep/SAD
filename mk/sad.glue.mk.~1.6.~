# New Makefile -- Library Glue Part
#

OBJGLUE_Xlib=
OBJGLUE_TkInter=
OBJGLUE_EPICS=dummyroutCA.o

ifeq ($(USE_X11),YES)
OBJGLUE_Xlib=tfXlib_.o
endif

ifeq ($(USE_TCLTK),YES)
ifeq ($(TCLTK_GUI_BACKEND),AQUA)
OBJGLUE_Xlib=tfXlib_.o
endif
endif

ifneq ($(OBJGLUE_Xlib),)
SAD_GLUE_DEF_FUNCS+=Xlib
endif

ifeq ($(USE_TCLTK),YES)
OBJGLUE_TkInter=tfTkInter_.o tfTclArg.o tfcanvasclip.o
SAD_GLUE_DEF_FUNCS+=TkInter
endif

ifeq ($(USE_EPICS),YES)
OBJGLUE_EPICS=tfefun2.o tfefun3ep.o CaSearch2.o DbStatic.o CaSearch_.o
endif

# correction point of sadDefFunc_* family
OBJGLUE=tfDefFuncs_.o

OBJGLUE+=$(OBJGLUE_Xlib) $(OBJGLUE_TkInter)
OBJGLUE+=$(OBJGLUE_EPICS)

ifeq ($(USE_PLFIT),YES)
OBJGLUE+= 
else
OBJGLUE+= dummyrout.o
endif

# End of File
